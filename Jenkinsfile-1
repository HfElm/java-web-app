pipeline {
  agent any  
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
          sh './mvnw clean verify sonar:sonar -Dsonar.login=$SONAR_TOKEN -Dsonar.projectKey=com.example:demo2'

        }
      }
    }
  }
}
